<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="BifrostConfig" type="Bifrost.Configuration.BifrostConfigHandler, Bifrost" />
  </configSections>
  <BifrostConfig>
  </BifrostConfig>
  <appSettings>
    <add key="Database" value="http://localhost:8080" />
    <add key="RavenUsername" value=""/>
    <add key="RavenPassword" value=""/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
